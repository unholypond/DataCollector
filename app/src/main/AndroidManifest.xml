<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.metroState.ics372GroupProject3.dataCollector">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="edu.metroState.ics372GroupProject3.dataCollector.ProgramState"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="edu.metroState.ics372GroupProject3.dataCollector.DisplayActivity"></activity>
        <activity
            android:name="edu.metroState.ics372GroupProject3.dataCollector.CreateActivity"
            android:windowSoftInputMode="stateHidden"/>
        <activity
            android:name="edu.metroState.ics372GroupProject3.dataCollector.ImportActivity"
            android:windowSoftInputMode="stateHidden"/>
        <activity
            android:name="edu.metroState.ics372GroupProject3.dataCollector.MainActivity"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>